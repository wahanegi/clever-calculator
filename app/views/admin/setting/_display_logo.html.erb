<div class="panel">
  <h3>Current logo</h3>
  <div class="panel_contents">
    <% if setting.logo.attached? %>
      <%= image_tag setting.logo, style: 'margin-bottom: 10px; object-fit: contain; width: 200px; max-height: 200px;' %>

      <%= button_to 'Remove logo', admin_setting_remove_logo_path, method: :delete %>
    <% else %>
      Logo not uploaded
    <% end %>
  </div>
</div>